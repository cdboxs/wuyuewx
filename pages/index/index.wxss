/**index.wxss**/
@import "../template/index.wxss";
page{
  overflow-y: hidden;
}
.search {
  width: 90%;
  height: auto;
  margin: 0 auto 30rpx;
  position: relative;
}

.search input {
  width: 100%;
  height: 65rpx;
  color: #666;
  line-height: 65rpx;
  margin: 0 auto;
  background: #f3f4f6;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 30rpx;
  border-radius: 33rpx;
  position: relative;
  display: flex;
  align-items: center;
}

.search image {
  display: block;
  width: 30px;
  height: 30px;
}
.searchResult{
  width:100%;
  overflow: hidden;
  background:#fefefe;
  border:1px solid #eee;
  position: absolute;top:65rpx;left:0;z-index: 99999;
}
.searchResult view{
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
}
.searchResult text{
  width:45%;
  height:80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  padding-left:20rpx;
  box-sizing: border-box;
  border-bottom:1rpx solid #eee;
}
.searchResult text:first-child{
  width:75%;
}
.searchResult text:last-child{
  width:25%;
}
.switch_nav {
  width: 100%;
  height: 60rpx;
  overflow: hidden;
  font-size: 32rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
}

.active {
  color: #76b5fc;
  border-bottom: 5rpx solid #76b5fc;
}

.switch_nav text {
  width: 33.3%;
  text-align: center;
}



