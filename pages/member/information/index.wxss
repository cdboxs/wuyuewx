/* pages/member/information/index.wxss */
page{
  background:#f0f3fc;
}
.information_photo{
  width:100%;
  padding:20rpx 30rpx;
  margin:30rpx 0;
  box-sizing: border-box;
  background:#fff;
  display: flex;
  justify-content: space-between;
}
.information_photo view:first-child{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.information_photo view:first-child image{
  width: 67px;
  height:67px;
  border-radius: 50%;
  margin-right: 20rpx;
}
.information_photo view:last-child{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.information_photo view:last-child image{
  width: 48rpx;
  height:48rpx;
  margin-left: 20rpx;
}


.information_menu{
  width:100%;
  padding:30rpx;
  box-sizing: border-box;
  background:#fff;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #eee;
}

.information_menu view:first-child{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}

.information_menu view:last-child{
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.information_menu view:last-child image{
  width: 48rpx;
  height:48rpx;
  margin-left: 20rpx;
}

.ueditorUserInfo{
  width:94%;
  height:100rpx;
  color:#fff;
  margin:50rpx auto;
}
























